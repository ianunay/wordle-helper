{
  "name": "Wordle Helper",
  "description": "A simple extension which provides suggestions in wordle",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.powerlanguage.co.uk/wordle/"],
      "js": ["content-script.js", "wordle-dict.js"]
    }
  ]
}